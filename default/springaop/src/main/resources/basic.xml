<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:os-core="http://www.openspaces.org/schema/core"
	xmlns:os-events="http://www.openspaces.org/schema/events"
	xmlns:os-remoting="http://www.openspaces.org/schema/remoting"
	xmlns:os-sla="http://www.openspaces.org/schema/sla"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.openspaces.org/schema/core http://www.openspaces.org/schema/core/openspaces-core.xsd
       http://www.openspaces.org/schema/events http://www.openspaces.org/schema/events/openspaces-events.xsd
       http://www.openspaces.org/schema/remoting http://www.openspaces.org/schema/remoting/openspaces-remoting.xsd
       http://www.openspaces.org/schema/sla http://www.openspaces.org/schema/sla/openspaces-sla.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
    xmlns:aop="http://www.springframework.org/schema/aop">
       <aop:aspectj-autoproxy/>
       <context:annotation-config/>
       
       <bean id="basicPointCuts" class="bonfire.springaop.aspects.BasicPointCuts"></bean>
       <bean id="basicAdvice" class="bonfire.springaop.aspects.BasicAdvice"></bean>
       <bean id="myBasicTarget" class="bonfire.springaop.targets.MyTestTarget"></bean>
       <bean name="mySaveTarget" class="bonfire.springaop.targets.MySaveTarget">
       </bean>
       <bean class="java.lang.String">
       		<constructor-arg value="testString"></constructor-arg>
       </bean>
</beans>